<com.profitles.framwork.cusviews.view.MyRelativeLayout xmlns:matt="http://schemas.android.com/apk/res/com.profitles.activity"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center|top"
    tools:context="com.org.profitles.LoginActivity" >

    <com.profitles.framwork.cusviews.view.MyLinearLayout
        android:id="@+id/myLinearLayout1"
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:gravity="center|top"
        android:orientation="vertical" >

        <com.profitles.framwork.cusviews.view.MyLinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center|top"
            android:orientation="vertical" >

            <com.profitles.framwork.cusviews.view.MyLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                    <com.profitles.framwork.cusviews.view.MyLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" >

                        <com.profitles.framwork.cusviews.view.MyTextView
                            style="@style/CodeFont"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:text="@string/txvBar"
                            android:textAppearance="?android:attr/textAppearanceLarge" />

                        <com.profitles.framwork.cusviews.view.MyReadBQ
                            android:id="@+id/etx_woQcOkScan"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            matt:isBlurRequired="true" >
                        </com.profitles.framwork.cusviews.view.MyReadBQ>
                    </com.profitles.framwork.cusviews.view.MyLinearLayout>

                    <com.profitles.framwork.cusviews.view.MyLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp" >

                        <com.profitles.framwork.cusviews.view.MyTextView
                            style="@style/CodeFont"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:text="日期："
                            android:textAppearance="?android:attr/textAppearanceLarge" />

                        <com.profitles.framwork.cusviews.view.MyEditText
                            android:id="@+id/etx_woQcOkWoDate"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:background="@null"
                            android:enabled="false" />

                        <com.profitles.framwork.cusviews.view.MyTextView
                            style="@style/CodeFont"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginLeft="20dp"
                            android:text="班次："
                            android:textAppearance="?android:attr/textAppearanceLarge" />

                        <com.profitles.framwork.cusviews.view.MyEditText
                            android:id="@+id/etx_woQcOkShift"
                            android:layout_width="92dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:background="@null"
                            android:enabled="false" />
                    </com.profitles.framwork.cusviews.view.MyLinearLayout>

                    <com.profitles.framwork.cusviews.view.MyLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp" >

                        <com.profitles.framwork.cusviews.view.MyTextView
                            style="@style/CodeFont"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:text="车间："
                            android:textAppearance="?android:attr/textAppearanceLarge" />

                        <com.profitles.framwork.cusviews.view.MyEditText
                            android:id="@+id/etx_woQcOkWkctr"
                            android:layout_width="54dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.35"
                            android:background="@null"
                            android:enabled="false" />

                        <com.profitles.framwork.cusviews.view.MyTextView
                            style="@style/CodeFont"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:text="产线："
                            android:textAppearance="?android:attr/textAppearanceLarge" />

                        <com.profitles.framwork.cusviews.view.MyEditText
                            android:id="@+id/etx_woQcOkLine"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginLeft="5dp"
                            android:layout_weight="0.39"
                            android:background="@null"
                            android:enabled="false" />
                    </com.profitles.framwork.cusviews.view.MyLinearLayout>

                    <com.profitles.framwork.cusviews.view.MyLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp" >

                        <com.profitles.framwork.cusviews.view.MyTextView
                            style="@style/CodeFont"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:text="加工单："
                            android:textAppearance="?android:attr/textAppearanceLarge" />

                        <com.profitles.framwork.cusviews.view.MyEditText
                            android:id="@+id/etx_woQcOkWoNbr"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@null"
                            android:enabled="false" />
                    </com.profitles.framwork.cusviews.view.MyLinearLayout>

                    <com.profitles.framwork.cusviews.view.MyLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp" >

                        <com.profitles.framwork.cusviews.view.MyTextView
                            style="@style/CodeFont"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:text="@string/txvPart"
                            android:textAppearance="?android:attr/textAppearanceLarge" />

                        <com.profitles.framwork.cusviews.view.MyEditText
                            android:id="@+id/etx_woQcOkPart"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@null"
                            android:enabled="false" />
                    </com.profitles.framwork.cusviews.view.MyLinearLayout>

                    <com.profitles.framwork.cusviews.view.MyLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="3" >

                        <com.profitles.framwork.cusviews.view.MyEditText
                            android:id="@+id/etx_woQcOkPartDesc"
                            android:layout_width="64dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="64dp"
                            android:layout_weight="0.35"
                            android:background="@null"
                            android:enabled="false" />
                    </com.profitles.framwork.cusviews.view.MyLinearLayout>

                    <com.profitles.framwork.cusviews.view.MyLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp" >

                        <com.profitles.framwork.cusviews.view.MyTextView
                            style="@style/CodeFont"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:text="工序："
                            android:textAppearance="?android:attr/textAppearanceLarge" />

                        <com.profitles.framwork.cusviews.view.MyEditText
                            android:id="@+id/etx_woQcOkOp"
                            android:layout_width="146dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_weight="0.28"
                            android:background="@null"
                            android:enabled="false" />
                    </com.profitles.framwork.cusviews.view.MyLinearLayout>

                    <com.profitles.framwork.cusviews.view.MyLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp" >

                        <com.profitles.framwork.cusviews.view.MyTextView
                            style="@style/CodeFont"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:text="数量："
                            android:textAppearance="?android:attr/textAppearanceLarge" />

                        <com.profitles.framwork.cusviews.view.MyEditText
                            android:id="@+id/etx_woQcOkQty"
                            android:layout_width="146dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_weight="0.28"
                            android:background="@null"
                            android:enabled="false" />
                    </com.profitles.framwork.cusviews.view.MyLinearLayout>
                </com.profitles.framwork.cusviews.view.MyLinearLayout>
            </com.profitles.framwork.cusviews.view.MyLinearLayout>
    </com.profitles.framwork.cusviews.view.MyLinearLayout>
</com.profitles.framwork.cusviews.view.MyRelativeLayout>